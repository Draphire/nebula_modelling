  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Web on Chrome",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["-d", "chrome", "--web-port", "3000"]
    },
    {
      "name": "nebula_modelling-1",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "nebula_modelling-1 (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "nebula_modelling-1 (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
